{
	"name": "mpx-vscode",
	"private": true,
	"version": "0.0.1",
	"icon": "images/icon.png",
	"displayName": "Mpx VSCode",
	"description": "Language Support for Mpx",
	"author": "wangshunnn",
	"publisher": "Mpx",
	"repository": {
		"type": "git",
		"url": "",
		"directory": "packages/vscode"
	},
	"categories": [
		"Programming Languages"
	], 
	"engines": {
		"vscode": "^1.88.0"
	},
	"main": "./client.js",
	"scripts": {
		"prebuild": "pnpm run postinstall && pnpm -w run build",
		"build": "node scripts/build",
		"build:minify": "pnpm run build --minify",
		"watch": "pnpm run build --watch",
		"pack": "pnpm run build:minify && vsce package",
		"pack:next": "pnpm run build && vsce package",
		"release": "pnpm run build:minify && vsce publish",
		"release:next": "pnpm run build && vsce publish --pre-release",
		"size": "pnpm run build:minify --metafile && esbuild-visualizer --metadata ./meta.json && open ./stats.html",
		"postinstall": "echo test"
	},
	"devDependencies": {
		"@types/semver": "^7.5.3",
		"@types/vscode": "^1.82.0",
		"@volar/vscode": "~2.4.11",
		"@vscode/vsce": "^3.2.1",
		"esbuild": "^0.25.0",
		"esbuild-visualizer": "^0.7.0",
		"reactive-vscode": "^0.2.9",
		"semver": "^7.5.4",
		"vscode-ext-gen": "^0.5.0",
		"vscode-tmlanguage-snapshot": "^0.1.3"
	}
}
